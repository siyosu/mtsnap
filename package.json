{
  "name": "mtsnap",
  "version": "0.1.4",
  "description": "Unofficial Midtrans snap payment API client for node",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "format": "prettier . --write",
    "typecheck": "tsc",
    "build": "tsup src/index.ts --format cjs,esm --minify --dts",
    "prepare": "rm -rf dist && npm run build"
  },
  "repository": "https://github.com/siyosu/mtsnap",
  "bugs": "https://github.com/siyosu/mtsnap/issues",
  "homepage": "https://github.com/siyosu/mtsnap#readme",
  "keywords": [
    "midtrans",
    "api",
    "payment",
    "gateway"
  ],
  "author": "Yosua Tarida Panjaitan",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "prettier": "^3.3.3",
    "tsup": "^8.1.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  }
}
